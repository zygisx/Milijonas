        <Label x:Name="label9" Content="10 000" HorizontalAlignment="Left" Height="19.751" Margin="17.486,45.921,0,0" Style="{DynamicResource LabelStyle1}" VerticalAlignment="Top" Width="102.514" Foreground="Black" FontFamily="MS Reference Sans Serif" FontSize="13.333"/>
		<Label x:Name="label10" HorizontalContentAlignment="Stretch" Content="20 000" Margin="0,199.672,22,0" Style="{DynamicResource LabelStyle2}" Foreground="Black" FontFamily="MS Reference Sans Serif" FontSize="13.333" Background="#00782626" HorizontalAlignment="Right" Width="63.014" Height="19.751" VerticalAlignment="Top"/>
		<Label x:Name="label11" HorizontalContentAlignment="Stretch" Content="40 000" Margin="0,179.921,22,0" Style="{DynamicResource LabelStyle2}" Foreground="Black" FontFamily="MS Reference Sans Serif" FontSize="13.333" Background="#00782626" HorizontalAlignment="Right" Width="72.014" Height="19.751" VerticalAlignment="Top"/>
